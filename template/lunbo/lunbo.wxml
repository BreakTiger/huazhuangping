<template name="lunbo">
  <swiper autoplay="true" circular="true" indicator-dots="true" indicator-color="rgba(255,255,255, 0.3)" indicator-active-color="#B80106">
    <block wx:for="{{banner}}" wx:key="banner">
      <swiper-item>
        <image src="{{item.url}}" class="slideImage" />
      </swiper-item>
    </block>
  </swiper>
</template>

