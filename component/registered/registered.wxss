.register{
   width: 100%;
   height: 100%;
   background: #fff;
   position: fixed;
   top: 0rpx;
   left: 0rpx;
   
}
.register .yes{
  width: 600rpx;
  height: 80rpx;
  background: rgba(184,1,6,1);
  border-radius: 12rpx;
  color: #fff;
  text-align: center;
  line-height: 80rpx;
  margin: auto;
  margin-top: 100rpx;
  z-index: 22;
  font-size: 28rpx
}
button::after{
  display: none
}
.register .no{
  font-size: 28rpx;
  width: 600rpx;
  height: 80rpx;
  border: 1rpx solid #B80106;
  background: #f8f8f8;
  border-radius: 12rpx;
  text-align: center;
  line-height: 80rpx;
  margin: auto;
  margin-top: 20rpx;
  z-index: 22;
  color: #B80106
}

.companyimg{
  width: 100%;
  height: 200rpx;
  margin: auto;
  margin-top: 200rpx;
  text-align: center
}
.companyimg image{
  width: 160rpx;
  height: 160rpx;
  border-radius: 50%
}